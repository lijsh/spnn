{
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": [
      2,
      "never"
    ],
    "eol-last": 0,
    "quotes": [
      2,
      "single"
    ],
    "no-underscore-dangle": [
      0
    ],
    "no-param-reassign": 0,
    "no-shadow": 0,
    "linebreak-style": 0,
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "consistent-return": 1
  }
}